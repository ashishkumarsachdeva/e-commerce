
 <!-- Header Start-->
 <%- include('./include/header.ejs') %>
  <!-- Header End-->
  <link href="/css/shop/paclageOrders.css" rel="stylesheet">
</head>

<body>

    <%- include('./sideBar.ejs') %>
        <!-- Page wrapper  -->
        <div class="page-wrapper">
            <!-- Bread crumb and right sidebar toggle -->
            <div class="row page-titles">
                <div class="col-md-5 col-12 align-self-center">
                    <h3 class="text-themecolor mb-0">Package Orders</h3>
                    <ol class="breadcrumb mb-0 p-0 bg-transparent">
                        <li class="breadcrumb-item"><a href="javascript:void(0)">Home</a></li>
                        <li class="breadcrumb-item active">Packages Orders</li>
                    </ol>
                </div>

            </div>
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12">
                        <table class="table table-bordered" id="editableTable">
                            <thead>
                                <tr class="text-center">
                                    <th>User Mail</th>
                                    <th>Packages Name</th>
                                    <th>Package Token</th>
                                    <th>service</th>
                                    <th>Price</th>
                                    <th>Tax</th>
                                    <th>Order Date</th>
                                    <th>Valid till</th>
                                    <th class="text-center">Approve</th>
                                    <th class="text-center">Delete</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% for(let i=0; i< data.length; i++) { %>
                                    <tr class="text-center">
                                        <th> <%=data[i].userMail %> </th>
                                        <th> <%=data[i].packageName %> </th>
                                        <th> <%=data[i].packageToken %> </th>
                                        <th> <%=data[i].service %> </th>
                                        <th> <%=data[i].price %> </th>
                                        <th> <%=data[i].tax %> </th>
                                        <th> <%=data[i].date %> </th>
                                        <th> <%=data[i].endDate %> </th>
                                        <th class="text-center"><a href="/shop/approve/package/<%=data[i].tokenOrder%>"><i class="fa fa-check"></i></a> </th>  
                                        <th class="text-center"><a href="/shop/delete/package/<%=data[i].tokenOrder%>"><i class="fa fa-trash text-danger"></i></a> </th>  
                                    </tr>
                                <% } %>
                            </tbody>
                        </table>
                    </div>
                </div>

            </div>
        </div>

        </div>
        <!-- End Wrapper -->
 <!-- JS Plugin Start-->
 <%- include('./include/js.ejs') %>
  <!-- JS Plugin End-->

</body>

</html>